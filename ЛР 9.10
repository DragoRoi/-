Блочная сортировка 
def bucket_sort(arr):
    # Создаем пустые корзины
    buckets = [[] for _ in range(len(arr))]
    
    # Распределяем элементы по корзинам
    for num in arr:
        index = int(num * len(arr))  # Для чисел 0-1
        buckets[index].append(num)
    
    # Сортируем корзины и объединяем
    return [num for bucket in buckets for num in sorted(bucket)]

# Пример
arr = [0.42, 0.32, 0.33, 0.52, 0.37, 0.47, 0.51]
print("Блочная сортировка:", bucket_sort(arr))

Бинная сортировка
def pancake_sort(arr):
    def flip(k):
        arr[:k+1] = arr[:k+1][::-1]
    
    for size in range(len(arr), 1, -1):
        # Находим максимум и переворачиваем
        max_idx = arr.index(max(arr[:size]))
        if max_idx != size-1:
            flip(max_idx)
            flip(size-1)
    return arr

# Пример
arr = [23, 10, 20, 11, 12, 6, 7]
print("Блинная сортировка:", pancake_sort(arr.copy()))

Сортировка бусинами
def bead_sort(arr):
    # Создаем "бусины"
    beads = [[1] * num for num in arr]
    
    # "Падаем" бусинами
    for i in range(len(beads[0])):
        # Считаем бусины в каждом столбце
        for j in range(len(beads)):
            beads[j][i] = 1 if i < arr[j] else 0
    
    # Преобразуем обратно в числа
    return [sum(row) for row in beads]

# Пример
arr = [3, 1, 4, 1, 2]
print("Сортировка бусинами:", bead_sort(arr))

Поиск скачками
import math

def jump_search(arr, target):
    n = len(arr)
    step = int(math.sqrt(n))
    prev = 0
    
    # Прыжки вперед
    while arr[min(step, n)-1] < target:
        prev = step
        step += int(math.sqrt(n))
        if prev >= n:
            return -1
    
    # Линейный поиск в блоке
    while arr[prev] < target:
        prev += 1
        if prev == min(step, n):
            return -1
    
    return prev if arr[prev] == target else -1

# Пример
arr = [1, 3, 5, 7, 9, 11, 13, 15]
target = 9
print(f"Поиск скачками: элемент {target} на позиции {jump_search(arr, target)}")

Экспонециальный поиск
def exponential_search(arr, target):
    if arr[0] == target:
        return 0
    
    # Находим диапазон
    i = 1
    while i < len(arr) and arr[i] <= target:
        i *= 2
    
    # Бинарный поиск в диапазоне
    left, right = i//2, min(i, len(arr)-1)
    while left <= right:
        mid = (left + right) // 2
        if arr[mid] == target:
            return mid
        elif arr[mid] < target:
            left = mid + 1
        else:
            right = mid - 1
    return -1

# Пример
arr = [2, 3, 4, 10, 40, 45, 50]
target = 10
print(f"Экспоненциальный поиск: элемент {target} на позиции {exponential_search(arr, target)}")

Тернарный поиск 
def ternary_search(arr, target):
    left, right = 0, len(arr)-1
    
    while left <= right:
        # Делим на 3 части
        mid1 = left + (right - left) // 3
        mid2 = right - (right - left) // 3
        
        if arr[mid1] == target:
            return mid1
        if arr[mid2] == target:
            return mid2
            
        if target < arr[mid1]:
            right = mid1 - 1
        elif target > arr[mid2]:
            left = mid2 + 1
        else:
            left, right = mid1 + 1, mid2 - 1
            
    return -1

# Пример
arr = [1, 2, 3, 4, 5, 6, 7, 8, 9]
target = 5
print(f"Тернарный поиск: элемент {target} на позиции {ternary_search(arr, target)}")
